<div class="x_panel" >
    <div class="x_title">
        <h2>lienfamilliales</h2>
        <ul class="nav navbar-right panel_toolbox">
            <li><a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
            </li>
            <li class="dropdown">
                <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-expanded="t"><i class="fa fa-wrench"></i></a>
            </li>
        </ul>
        <div class="clearfix"></div>
    </div>
    <div class="x_content" style="display: block;">
        <br>
        <table id="datansive" class="table table-striped table-bordered dt-responsive nowrap" cellspacing="0" width="100%">
            <thead>
            <tr>
                <th></th>
                <th>LIEN</th>
                <th>NOM</th>
                <th>PRENOM</th>
                <th>PROFESSION</th>
                <th>DOMICILE</th>
                <th>EMAIL</th>
                <th>TEL</th>
                <th>TEL WHATSAPP</th>
                <th>Action</th>
            </tr>
            </thead>
            <tbody>
            {% for lienfamilliale in lienfamilliales %}
                <tr  >
                    <td></td>
                    <td>
                        {{ lienfamilliale.nominationlien}}
                    </td>
                    <td>
                        {{ lienfamilliale.tuteur.nom}}
                    </td>
                    <td>
                        {{ lienfamilliale.tuteur.nom }}
                    </td>
                    <td>
                        {% if  lienfamilliale.tuteur.profession is empty %}
                            <i class="fa fa-home red"></i>
                        {% else %}
                            {{ lienfamilliale.tuteur.profession }}
                        {% endif %}
                    </td>
                    <td>
                        {{ lienfamilliale.tuteur.domicile }}
                    </td>
                    <td>
                        {% if   lienfamilliale.tuteur.email is empty %}
                            <i class="fa fa-envelope-square red"></i>
                        {% else %}
                            {{ lienfamilliale.tuteur.email }}
                        {% endif %}
                    </td>
                    <td>
                        {% if  lienfamilliale.tuteur.tel is empty %}
                            <i class="fa fa-file-photo-o red"></i>
                        {% else %}
                            {{ lienfamilliale.tuteur.tel }}
                        {% endif %}
                    </td>
                    <td>
                        {{ lienfamilliale.tuteur.telwhat }}
                    </td>

                    <td>
                        <a href="{{ path('tuteur_edit', {'id':lienfamilliale.tuteur.id}) }}" class=" btn btn-info btn-xs"><i class="fa fa-edit"></i>  </a>
                        <a href="#" class=" btn btn-danger btn-xs"><i class="fa fa-eye"></i>  </a>
                    </td>
                </tr>
            {% endfor %}
            </tbody>
        </table>

    </div>
</div >