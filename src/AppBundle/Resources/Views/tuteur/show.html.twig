{% extends '@App/user_base.html.twig' %}

{% block title %} {{ parent() }} Consulter Utilisateurs {% endblock %}
{% block main %}
    <h1>Tuteur</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ tuteur.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ tuteur.nom }}</td>
            </tr>
            <tr>
                <th>Prenom</th>
                <td>{{ tuteur.prenom }}</td>
            </tr>
            <tr>
                <th>Telwhat</th>
                <td>{{ tuteur.telwhat }}</td>
            </tr>
            <tr>
                <th>Tel</th>
                <td>{{ tuteur.tel }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ tuteur.email }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('tuteur_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('tuteur_edit', { 'id': tuteur.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
